{
  "name": "elearn",
  "version": "0.0.1",
  "description": "Platform for E-Learning",
  "main": "server.js",
  "scripts": {
    "start": "babel --presets=react,es2015 server.js -o comp.server.js && node comp.server.js",
    "start-dev": "webpack-dev-server --hot --inline",
    "bundle": "webpack",
    "start-server": "pg-migrate up && nodemon server/index.js --exec babel-node --presets=react,es2015",
    "drop-tables": "pg-migrate down"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "node-pg-migrate": "^1.2.2",
    "path": "^0.12.7",
    "pg-promise": "^5.6.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "nodemon": "^1.11.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
